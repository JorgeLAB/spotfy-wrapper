{
  "name": "spotfy-wrapper",
  "version": "1.0.0",
  "description": "lib for search information with api spotify",
  "private": true,
  "scripts": {
    "lint": "./node_modules/.bin/eslint src/*.js",
    "test": "./node_modules/mocha/bin/mocha ./tests/*.spec.js",
    "test:tdd": "./node_modules/mocha/bin/mocha ./tests/*.spec.js --watch",
    "test:coverage": "nyc npm run test --reporter=lcov",
    "start": "webpack serve --mode development"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run lint"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JorgeLAB/spotfy-wrapper.git"
  },
  "keywords": [
    "music",
    "spotify",
    "lib",
    "tests",
    "search"
  ],
  "author": "Jorge Borges",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JorgeLAB/spotfy-wrapper/issues"
  },
  "homepage": "https://github.com/JorgeLAB/spotfy-wrapper#readme",
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/node": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/register": "^7.12.10",
    "babel-cli": "^6.26.0",
    "babel-loader": "^8.2.2",
    "chai": "^4.2.0",
    "eslint": "^7.17.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^5.0.6",
    "lodash": "^4.17.20",
    "mocha": "^8.2.1",
    "node-babel": "^0.1.2",
    "nyc": "^15.1.0",
    "prettier": "^2.2.1",
    "sinon": "^4.5.0",
    "sinon-chai": "^3.5.0",
    "sinon-stub-promise": "^4.0.0",
    "webpack": "^5.12.1",
    "webpack-cli": "^4.3.1",
    "webpack-dev-server": "^3.11.1"
  },
  "dependencies": {
    "regenerator-runtime": "^0.13.7",
    "core-js": "^3.8.2",
    "node-fetch": "^2.6.1"
  }
}
